@using Web.Models
<section id="intro">
    @Html.Raw(Cache.Intro)
</section>
<hr />
<section id="blog">
    <h4>Recent Blog Posts</h4>
    <ul class="list-unstyled">
        @foreach (Page post in Cache.FrontPagePosts)
        {
            <li class="post-summary">
                <time>@post.Timestamp.ToString("MMM d")</time>
                <a class="title" href="/blog/@post.URL.ToLower()"><b>@post.Title</b></a>
                <h4 class="description"><small>@post.Description</small></h4>
            </li>
        }
    </ul>
    <a href="/blog"><i>(more)</i></a>
</section>
<hr />
<section id="photos">
    <h4>Recent Photos</h4>
    @Html.Partial("~/Views/Photo/Gallery", Cache.FrontPageImages)
    <a href="/photo"><i>(more)</i></a>
</section>