@model IQueryable<web.Models.Image>
<div class="gallery">
    <div class="container-fluid">
        @foreach (var image in Model)
        {
            <a href="/photo/view/@image.ID" class="thumbnail-image" style="background-image: url('/images/@{@image.Name}-0240.jpg')">&nbsp;</a>
    }
    </div>
</div>