@model Web.Models.Image

<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:description" content="@ViewBag.Subtitle"/>
<meta name="twitter:image" content="http://stevedesmond.ca/images/@Model.Name-0960.jpg"/>

<h3 class="text-center">@ViewBag.Subtitle</h3>
<rimg base="@Model.Name" alt="@string.Join("; ", Model.ImageTags.Select(it => it.Tag).Select(t => t.Name))" />