@using VTSV.Models
@model IList<Image>

@Html.Action("Navbar", "Tags", new { images = Model })

@Html.Partial("~/Views/Images/Lightbox.cshtml")

<div class="gallery container-fluid">
    @(ViewBag.Featured != null
    ? Html.Action("Featured", "Images", new { featured = Model.First() })
    : Html.Action("Gallery", "Images"))

    <div class="clear-both"></div>
    <div class="fb-like" data-href="https://www.facebook.com/stevedesmondphotography" data-layout="button" data-action="like" data-show-faces="false" data-share="false"></div>
</div>